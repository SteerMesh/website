{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "out",
    "cleanUrls": true,
    "trailingSlash": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/waitlist",
        "function": "waitlist"
      }
    ]
  },
  "firestore": {
    "rules": "firestore.rules"
  }
}
